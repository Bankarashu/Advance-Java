<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.bean.EmployeeBean1"
		name="employee1">

		<property name="id" value="1" />
		<property name="name" value="Roshani" />
		<property name="salary" value="35000" />
		<property name="companys" ref="companys" />
	</bean>

	<bean class="com.jspiders.springcorexml.bean.EmployeeBean1"
		name="employee2">

		<property name="id" value="2" />
		<property name="name" value="Ashu" />
		<property name="salary" value="85000" />
		<property name="companys" ref="companys" />
	</bean>

	<bean class="com.jspiders.springcorexml.bean.CompanyBean1"
		name="companys">
		<property name="id" value="1" />
		<property name="name" value="TCS" />
		<property name="location" value="Mumbai" />

	</bean>

</beans>